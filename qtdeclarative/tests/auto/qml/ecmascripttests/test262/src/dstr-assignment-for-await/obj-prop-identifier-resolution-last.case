let x = null;
let w;
//- elems
{ w, a: x }
//- vals
{ a: 4 }
//- body
assert.sameValue(x, 4);
//- teardown
promise
  .then(() => assert.sameValue(iterCount, 1, 'iteration occurred as expected'), $DONE)
  .then($DONE, $DONE);
