<scxml xmlns="http://www.w3.org/2005/07/scxml"
    version="1.0"
    name="ColorStepper"
    initial="red">

    <state id = "red">
        <transition target="yellow" event="step"/>
    </state>

    <state id = "yellow">
      <transition target="green" event="step"/>
    </state>

    <final id="green"/>
</scxml>
