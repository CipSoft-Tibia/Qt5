  <xqx:mainModule>
    <xqx:prolog>
      <xqx:schemaImport>
        <xqx:namespacePrefix>myType</xqx:namespacePrefix>
        <xqx:targetNamespace>http://www.w3.org/XQueryTest/userDefinedTypes</xqx:targetNamespace>
      </xqx:schemaImport>
      <xqx:varDecl>
        <xqx:varName>input-context1</xqx:varName>
        <xqx:external/>
