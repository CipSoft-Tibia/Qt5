              <xqx:valueExpr>
                <xqx:sequenceExpr/>
              </xqx:valueExpr>
            </xqx:computedAttributeConstructor>
            <xqx:computedAttributeConstructor>
              <xqx:tagName>name3</xqx:tagName>
              <xqx:valueExpr>
                <xqx:sequenceExpr/>
              </xqx:valueExpr>
            </xqx:computedAttributeConstructor>
          </xqx:sequenceExpr>
        </xqx:varValue>
      </xqx:varDecl>
      <xqx:varDecl>
        <xqx:varName>b</xqx:varName>
        <xqx:typeDeclaration>
          <xqx:attributeTest/>
          <xqx:occurrenceIndicator>*</xqx:occurrenceIndicator>
        </xqx:typeDeclaration>
        <xqx:varValue>
          <xqx:sequenceExpr>
            <xqx:computedAttributeConstructor>
              <xqx:tagName>name1</xqx:tagName>
              <xqx:valueExpr>
