    </xqx:prolog>
    <xqx:queryBody>
      <xqx:unorderedExpr>
        <xqx:argExpr>
          <xqx:unionOp>
            <xqx:firstOperand>
              <xqx:pathExpr>
                <xqx:stepExpr>
                  <xqx:filterExpr>
                    <xqx:varRef>
                      <xqx:name>input-context</xqx:name>
