              </xqx:varRef>
            </xqx:arguments>
          </xqx:functionCallExpr>
        </xqx:varValue>
      </xqx:varDecl>
      <xqx:varDecl>
        <xqx:varName>lineWidth</xqx:varName>
        <xqx:typeDeclaration>
          <xqx:atomicType xqx:prefix="xs">integer</xqx:atomicType>
        </xqx:typeDeclaration>
        <xqx:varValue>
          <xqx:integerConstantExpr>
            <xqx:value>70</xqx:value>
          </xqx:integerConstantExpr>
        </xqx:varValue>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:elementConstructor>
        <xqx:tagName>allCodepoints</xqx:tagName>
        <xqx:elementContent>
          <xqx:computedCommentConstructor>
            <xqx:argExpr>
              <xqx:stringConstantExpr>
                <xqx:value> Each &lt;r&gt;-element represents a codepoint range. The 's' attribute
         is the start codepoint, the 'e' attribute is the end codepoint.
         Note that these are only *Hints*, since the character range is not contiguous.
      </xqx:value>
              </xqx:stringConstantExpr>
            </xqx:argExpr>
          </xqx:computedCommentConstructor>
          <xqx:sequenceExpr>
            <xqx:stringConstantExpr>
              <xqx:value>&#xA;</xqx:value>
            </xqx:stringConstantExpr>
            <xqx:stringConstantExpr>
              <xqx:value>&#xA;</xqx:value>
            </xqx:stringConstantExpr>
            <xqx:flworExpr>
              <xqx:forClause>
                <xqx:forClauseItem>
                  <xqx:typedVariableBinding>
                    <xqx:varName>i</xqx:varName>
                  </xqx:typedVariableBinding>
                  <xqx:forExpr>
                    <xqx:rangeSequenceExpr>
                      <xqx:startExpr>
                        <xqx:integerConstantExpr>
                          <xqx:value>1</xqx:value>
                        </xqx:integerConstantExpr>
                      </xqx:startExpr>
                      <xqx:endExpr>
                        <xqx:idivOp>
                          <xqx:firstOperand>
                            <xqx:varRef>
                              <xqx:name>count</xqx:name>
                            </xqx:varRef>
                          </xqx:firstOperand>
                          <xqx:secondOperand>
                            <xqx:varRef>
                              <xqx:name>lineWidth</xqx:name>
                            </xqx:varRef>
                          </xqx:secondOperand>
                        </xqx:idivOp>
                      </xqx:endExpr>
                    </xqx:rangeSequenceExpr>
                  </xqx:forExpr>
                </xqx:forClauseItem>
              </xqx:forClause>
              <xqx:letClause>
                <xqx:letClauseItem>
                  <xqx:typedVariableBinding>
                    <xqx:varName>startOffset</xqx:varName>
                  </xqx:typedVariableBinding>
                  <xqx:letExpr>
                    <xqx:addOp>
                      <xqx:firstOperand>
                        <xqx:multiplyOp>
                          <xqx:firstOperand>
                            <xqx:subtractOp>
                              <xqx:firstOperand>
                                <xqx:varRef>
                                  <xqx:name>i</xqx:name>
                                </xqx:varRef>
                              </xqx:firstOperand>
                              <xqx:secondOperand>
                                <xqx:integerConstantExpr>
                                  <xqx:value>1</xqx:value>
                                </xqx:integerConstantExpr>
                              </xqx:secondOperand>
                            </xqx:subtractOp>
                          </xqx:firstOperand>
                          <xqx:secondOperand>
                            <xqx:varRef>
                              <xqx:name>lineWidth</xqx:name>
                            </xqx:varRef>
                          </xqx:secondOperand>
                        </xqx:multiplyOp>
                      </xqx:firstOperand>
                      <xqx:secondOperand>
                        <xqx:integerConstantExpr>
                          <xqx:value>1</xqx:value>
                        </xqx:integerConstantExpr>
                      </xqx:secondOperand>
                    </xqx:addOp>
                  </xqx:letExpr>
                </xqx:letClauseItem>
              </xqx:letClause>
              <xqx:returnClause>
                <xqx:sequenceExpr>
                  <xqx:elementConstructor>
                    <xqx:tagName>r</xqx:tagName>
                    <xqx:attributeList>
                      <xqx:attributeConstructor>
                        <xqx:attributeName>s</xqx:attributeName>
                        <xqx:attributeValueExpr>
                          <xqx:pathExpr>
                            <xqx:stepExpr>
                              <xqx:filterExpr>
                                <xqx:varRef>
                                  <xqx:name>codepoints</xqx:name>
                                </xqx:varRef>
                              </xqx:filterExpr>
                              <xqx:predicates>
                                <xqx:varRef>
                                  <xqx:name>startOffset</xqx:name>
                                </xqx:varRef>
                              </xqx:predicates>
                            </xqx:stepExpr>
                          </xqx:pathExpr>
                        </xqx:attributeValueExpr>
                      </xqx:attributeConstructor>
                      <xqx:attributeConstructor>
                        <xqx:attributeName>e</xqx:attributeName>
                        <xqx:attributeValueExpr>
                          <xqx:addOp>
                            <xqx:firstOperand>
                              <xqx:pathExpr>
                                <xqx:stepExpr>
                                  <xqx:filterExpr>
                                    <xqx:varRef>
                                      <xqx:name>codepoints</xqx:name>
                                    </xqx:varRef>
                                  </xqx:filterExpr>
                                  <xqx:predicates>
                                    <xqx:varRef>
                                      <xqx:name>startOffset</xqx:name>
                                    </xqx:varRef>
                                  </xqx:predicates>
                                </xqx:stepExpr>
                              </xqx:pathExpr>
                            </xqx:firstOperand>
                            <xqx:secondOperand>
                              <xqx:varRef>
                                <xqx:name>lineWidth</xqx:name>
                              </xqx:varRef>
                            </xqx:secondOperand>
                          </xqx:addOp>
                        </xqx:attributeValueExpr>
                      </xqx:attributeConstructor>
                    </xqx:attributeList>
                    <xqx:elementContent>
                      <xqx:functionCallExpr>
                        <xqx:functionName>codepoints-to-string</xqx:functionName>
                        <xqx:arguments>
                          <xqx:functionCallExpr>
                            <xqx:functionName>subsequence</xqx:functionName>
                            <xqx:arguments>
                              <xqx:varRef>
                                <xqx:name>codepoints</xqx:name>
                              </xqx:varRef>
                              <xqx:varRef>
                                <xqx:name>startOffset</xqx:name>
                              </xqx:varRef>
                              <xqx:varRef>
                                <xqx:name>lineWidth</xqx:name>
                              </xqx:varRef>
                            </xqx:arguments>
                          </xqx:functionCallExpr>
                        </xqx:arguments>
                      </xqx:functionCallExpr>
                    </xqx:elementContent>
                  </xqx:elementConstructor>
                  <xqx:stringConstantExpr>
                    <xqx:value>&#xA;</xqx:value>
                  </xqx:stringConstantExpr>
                </xqx:sequenceExpr>
              </xqx:returnClause>
            </xqx:flworExpr>
          </xqx:sequenceExpr>
        </xqx:elementContent>
      </xqx:elementConstructor>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
