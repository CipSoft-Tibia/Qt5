            xsi:schemaLocation="http://www.w3.org/2005/XQueryX
                                http://www.w3.org/2005/XQueryX/xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:namespaceDecl>
        <xqx:prefix>functx</xqx:prefix>
        <xqx:uri>http://www.example.com/</xqx:uri>
      </xqx:namespaceDecl>
      <xqx:functionDecl>
        <xqx:functionName xqx:prefix="functx">duration-from-timezone</xqx:functionName>
        <xqx:paramList>
          <xqx:param>
            <xqx:varName>timezone</xqx:varName>
            <xqx:typeDeclaration>
              <xqx:atomicType xqx:prefix="xs">string</xqx:atomicType>
            </xqx:typeDeclaration>
          </xqx:param>
        </xqx:paramList>
        <xqx:typeDeclaration>
          <xqx:atomicType xqx:prefix="xs">dayTimeDuration</xqx:atomicType>
        </xqx:typeDeclaration>
        <xqx:functionBody>
          <xqx:functionCallExpr>
            <xqx:functionName xqx:prefix="xs">dayTimeDuration</xqx:functionName>
            <xqx:arguments>
              <xqx:ifThenElseExpr>
                <xqx:ifClause>
                  <xqx:functionCallExpr>
                    <xqx:functionName>not</xqx:functionName>
                    <xqx:arguments>
                      <xqx:functionCallExpr>
                        <xqx:functionName>matches</xqx:functionName>
                        <xqx:arguments>
                          <xqx:varRef>
                            <xqx:name>timezone</xqx:name>
                          </xqx:varRef>
                          <xqx:stringConstantExpr>
                            <xqx:value>Z|[\+\-]\d{2}:\d{2}</xqx:value>
                          </xqx:stringConstantExpr>
                        </xqx:arguments>
                      </xqx:functionCallExpr>
                    </xqx:arguments>
                  </xqx:functionCallExpr>
                </xqx:ifClause>
                <xqx:thenClause>
                  <xqx:functionCallExpr>
                    <xqx:functionName>error</xqx:functionName>
                    <xqx:arguments>
                      <xqx:functionCallExpr>
                        <xqx:functionName xqx:prefix="xs">QName</xqx:functionName>
                        <xqx:arguments>
                          <xqx:stringConstantExpr>
                            <xqx:value>functx:Invalid_Timezone_Value</xqx:value>
                          </xqx:stringConstantExpr>
                        </xqx:arguments>
                      </xqx:functionCallExpr>
                    </xqx:arguments>
                  </xqx:functionCallExpr>
                </xqx:thenClause>
                <xqx:elseClause>
                  <xqx:ifThenElseExpr>
                    <xqx:ifClause>
                      <xqx:equalOp>
                        <xqx:firstOperand>
                          <xqx:varRef>
                            <xqx:name>timezone</xqx:name>
                          </xqx:varRef>
                        </xqx:firstOperand>
                        <xqx:secondOperand>
                          <xqx:stringConstantExpr>
                            <xqx:value>Z</xqx:value>
                          </xqx:stringConstantExpr>
                        </xqx:secondOperand>
                      </xqx:equalOp>
                    </xqx:ifClause>
                    <xqx:thenClause>
                      <xqx:stringConstantExpr>
                        <xqx:value>PT0S</xqx:value>
                      </xqx:stringConstantExpr>
                    </xqx:thenClause>
                    <xqx:elseClause>
                      <xqx:functionCallExpr>
                        <xqx:functionName>replace</xqx:functionName>
                        <xqx:arguments>
                          <xqx:varRef>
                            <xqx:name>timezone</xqx:name>
                          </xqx:varRef>
                          <xqx:stringConstantExpr>
                            <xqx:value>\+?(\d{2}):\d{2}</xqx:value>
                          </xqx:stringConstantExpr>
                          <xqx:stringConstantExpr>
                            <xqx:value>PT$1H</xqx:value>
                          </xqx:stringConstantExpr>
                        </xqx:arguments>
                      </xqx:functionCallExpr>
                    </xqx:elseClause>
                  </xqx:ifThenElseExpr>
