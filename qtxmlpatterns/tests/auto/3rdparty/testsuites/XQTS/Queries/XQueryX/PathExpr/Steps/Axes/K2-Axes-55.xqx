            xsi:schemaLocation="http://www.w3.org/2005/XQueryX
                                http://www.w3.org/2005/XQueryX/xqueryx.xsd">
  <xqx:mainModule>
    <xqx:queryBody>
      <xqx:functionCallExpr>
        <xqx:functionName>empty</xqx:functionName>
        <xqx:arguments>
          <xqx:pathExpr>
            <xqx:stepExpr>
              <xqx:filterExpr>
                <xqx:sequenceExpr>
                  <xqx:sequenceExpr/>
                </xqx:sequenceExpr>
              </xqx:filterExpr>
            </xqx:stepExpr>
            <xqx:stepExpr>
              <xqx:xpathAxis>attribute</xqx:xpathAxis>
              <xqx:nameTest>attr</xqx:nameTest>
            </xqx:stepExpr>
          </xqx:pathExpr>
        </xqx:arguments>
      </xqx:functionCallExpr>
    </xqx:queryBody>
  </xqx:mainModule>
