  <xqx:mainModule>
    <xqx:prolog>
      <xqx:varDecl>
        <xqx:varName>input-context1</xqx:varName>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:functionCallExpr>
        <xqx:functionName xqx:prefix="fn">string-join</xqx:functionName>
        <xqx:arguments>
          <xqx:sequenceExpr>
            <xqx:functionCallExpr>
              <xqx:functionName xqx:prefix="fn">string</xqx:functionName>
              <xqx:arguments>
                <xqx:functionCallExpr>
                  <xqx:functionName xqx:prefix="fn">contains</xqx:functionName>
                  <xqx:arguments>
                    <xqx:functionCallExpr>
                      <xqx:functionName xqx:prefix="fn">document-uri</xqx:functionName>
                      <xqx:arguments>
                        <xqx:functionCallExpr>
                          <xqx:functionName xqx:prefix="fn">doc</xqx:functionName>
                          <xqx:arguments>
                            <xqx:varRef>
                              <xqx:name>input-context1</xqx:name>
                            </xqx:varRef>
                          </xqx:arguments>
                        </xqx:functionCallExpr>
                      </xqx:arguments>
                    </xqx:functionCallExpr>
                    <xqx:varRef>
                      <xqx:name>input-context1</xqx:name>
                    </xqx:varRef>
                  </xqx:arguments>
                </xqx:functionCallExpr>
              </xqx:arguments>
            </xqx:functionCallExpr>
            <xqx:stringConstantExpr>
              <xqx:value> A String</xqx:value>
            </xqx:stringConstantExpr>
          </xqx:sequenceExpr>
          <xqx:stringConstantExpr>
            <xqx:value></xqx:value>
          </xqx:stringConstantExpr>
        </xqx:arguments>
      </xqx:functionCallExpr>
    </xqx:queryBody>
  </xqx:mainModule>
