            xsi:schemaLocation="http://www.w3.org/2005/XQueryX
                                http://www.w3.org/2005/XQueryX/xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:varDecl>
        <xqx:varName>myVar</xqx:varName>
        <xqx:typeDeclaration>
          <xqx:atomicType xqx:prefix="xs">gYear</xqx:atomicType>
        </xqx:typeDeclaration>
        <xqx:varValue>
          <xqx:integerConstantExpr>
            <xqx:value>2006</xqx:value>
          </xqx:integerConstantExpr>
        </xqx:varValue>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:functionCallExpr>
        <xqx:functionName>true</xqx:functionName>
        <xqx:arguments>
        </xqx:arguments>
      </xqx:functionCallExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
