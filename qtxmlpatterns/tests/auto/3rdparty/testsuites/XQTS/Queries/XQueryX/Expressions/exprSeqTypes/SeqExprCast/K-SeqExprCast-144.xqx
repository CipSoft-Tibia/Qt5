              </xqx:functionCallExpr>
            </xqx:arguments>
          </xqx:functionCallExpr>
        </xqx:firstOperand>
        <xqx:secondOperand>
          <xqx:decimalConstantExpr>
            <xqx:value>1.1</xqx:value>
          </xqx:decimalConstantExpr>
        </xqx:secondOperand>
      </xqx:eqOp>
