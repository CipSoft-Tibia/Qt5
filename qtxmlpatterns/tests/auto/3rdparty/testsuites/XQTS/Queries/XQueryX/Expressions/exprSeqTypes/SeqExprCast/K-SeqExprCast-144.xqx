            <xqx:functionName xqx:prefix="xs">decimal</xqx:functionName>
            <xqx:arguments>
              <xqx:functionCallExpr>
                <xqx:functionName>remove</xqx:functionName>
                <xqx:arguments>
                  <xqx:sequenceExpr>
                    <xqx:doubleConstantExpr>
