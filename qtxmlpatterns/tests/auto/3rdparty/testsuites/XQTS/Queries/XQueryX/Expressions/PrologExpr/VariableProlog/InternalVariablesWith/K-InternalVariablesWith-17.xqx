  <xqx:mainModule>
    <xqx:prolog>
      <xqx:varDecl>
        <xqx:varName>var</xqx:varName>
        <xqx:varValue>
          <xqx:functionCallExpr>
            <xqx:functionName xqx:prefix="local">func1</xqx:functionName>
            <xqx:arguments>
            </xqx:arguments>
          </xqx:functionCallExpr>
        </xqx:varValue>
      </xqx:varDecl>
      <xqx:functionDecl>
        <xqx:functionName xqx:prefix="local">func1</xqx:functionName>
        <xqx:paramList/>
        <xqx:functionBody>
          <xqx:functionCallExpr>
            <xqx:functionName xqx:prefix="local">func2</xqx:functionName>
