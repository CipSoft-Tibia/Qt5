          <xqx:param>
            <xqx:varName>arg2</xqx:varName>
          </xqx:param>
        </xqx:paramList>
        <xqx:functionBody>
          <xqx:integerConstantExpr>
            <xqx:value>1</xqx:value>
          </xqx:integerConstantExpr>
        </xqx:functionBody>
      </xqx:functionDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:functionCallExpr>
        <xqx:functionName>true</xqx:functionName>
        <xqx:arguments>
        </xqx:arguments>
      </xqx:functionCallExpr>
    </xqx:queryBody>
