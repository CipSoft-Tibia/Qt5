                                          <xqx:stepExpr>
                                            <xqx:xpathAxis>child</xqx:xpathAxis>
                                            <xqx:nameTest>userid</xqx:nameTest>
                                          </xqx:stepExpr>
                                        </xqx:pathExpr>
                                      </xqx:firstOperand>
                                      <xqx:secondOperand>
                                        <xqx:pathExpr>
                                          <xqx:stepExpr>
                                            <xqx:filterExpr>
                                              <xqx:varRef>
                                                <xqx:name>u</xqx:name>
                                              </xqx:varRef>
                                            </xqx:filterExpr>
                                          </xqx:stepExpr>
                                          <xqx:stepExpr>
                                            <xqx:xpathAxis>child</xqx:xpathAxis>
                                            <xqx:nameTest>userid</xqx:nameTest>
                                          </xqx:stepExpr>
