                                      </xqx:secondOperand>
                                    </xqx:equalOp>
                                  </xqx:predicates>
                                </xqx:stepExpr>
                                <xqx:stepExpr>
                                  <xqx:xpathAxis>child</xqx:xpathAxis>
                                  <xqx:nameTest>itemno</xqx:nameTest>
                                </xqx:stepExpr>
