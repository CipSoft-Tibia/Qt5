                            <xqx:arguments>
                              <xqx:varRef>
                                <xqx:name>descr</xqx:name>
                              </xqx:varRef>
                            </xqx:arguments>
                          </xqx:functionCallExpr>
                        </xqx:orderByExpr>
                      </xqx:orderBySpec>
                    </xqx:orderByClause>
