                </xqx:functionCallExpr>
              </xqx:firstOperand>
              <xqx:secondOperand>
                <xqx:integerConstantExpr>
                  <xqx:value>1</xqx:value>
                </xqx:integerConstantExpr>
              </xqx:secondOperand>
            </xqx:subtractOp>
          </xqx:predicates>
        </xqx:stepExpr>
      </xqx:pathExpr>
    </xqx:queryBody>
  </xqx:mainModule>
