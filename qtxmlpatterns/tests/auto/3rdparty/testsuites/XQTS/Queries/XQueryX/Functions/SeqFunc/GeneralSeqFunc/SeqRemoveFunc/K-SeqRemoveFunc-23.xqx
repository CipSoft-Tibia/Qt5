  <xqx:mainModule>
    <xqx:queryBody>
      <xqx:pathExpr>
        <xqx:stepExpr>
          <xqx:filterExpr>
            <xqx:functionCallExpr>
              <xqx:functionName>remove</xqx:functionName>
              <xqx:arguments>
                <xqx:sequenceExpr>
                  <xqx:integerConstantExpr>
                    <xqx:value>1</xqx:value>
                  </xqx:integerConstantExpr>
                  <xqx:integerConstantExpr>
                    <xqx:value>2</xqx:value>
                  </xqx:integerConstantExpr>
                  <xqx:integerConstantExpr>
                    <xqx:value>3</xqx:value>
                  </xqx:integerConstantExpr>
                  <xqx:functionCallExpr>
                    <xqx:functionName>current-time</xqx:functionName>
                    <xqx:arguments>
                    </xqx:arguments>
                  </xqx:functionCallExpr>
                </xqx:sequenceExpr>
                <xqx:integerConstantExpr>
                  <xqx:value>9</xqx:value>
                </xqx:integerConstantExpr>
              </xqx:arguments>
            </xqx:functionCallExpr>
          </xqx:filterExpr>
          <xqx:predicates>
            <xqx:subtractOp>
              <xqx:firstOperand>
                <xqx:functionCallExpr>
                  <xqx:functionName>last</xqx:functionName>
                  <xqx:arguments>
