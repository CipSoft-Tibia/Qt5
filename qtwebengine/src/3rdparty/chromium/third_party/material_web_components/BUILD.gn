ts_library("bundle_lit_ts") {
  tsconfig_base = "tsconfig_base.json"
  definitions = [ "lit_exports.d.ts" ]
  composite = true

  # Client code that runs through optimize_webui will attempt to find lit
  # resources in $chrome_resources_dir. As such if ts code depends on mwc
  # it also needs to depend on the rules that move lit resources into
  # $chrome_resources_dir.
  extra_deps = [
    ":copy_lit_bundle",
    ":copy_tslib",
  ]
}

js_library("bundle_lit_js") {
  sources = [ "lit_exports.js" ]
}

rewrite_imports("mwc_rewrite") {
  base_dir =
      "//third_party/material_web_components/components-chromium/node_modules"
  out_dir = chrome_resources_dir

  import_mappings = [ "lit|//resources/mwc/lit/index.js" ]
  import_var_mappings = [
    "lit/static-html:html|staticHtml",
    "lit/static-html:svg|staticSvg",
  ]

  in_files = material_files

  deps = [ ":library" ]
}

# Compile lit_exports.js into a singular bundle we can host on
# chrome://resources.
optimize_webui("bundle_lit") {
  host = "resources"
  input = rebase_path("//third_party/material_web_components/", root_build_dir)
  js_module_in_files = [ "lit_exports.js" ]

  deps = [ ":bundle_lit_js" ]
}

copy("copy_tslib") {
  sources = [ "//third_party/material_web_components/components-chromium/node_modules/tslib/tslib.js" ]
  outputs = [ "$chrome_resources_dir/tslib/tslib.js" ]
}

copy("copy_lit_bundle") {
  sources = [ "$target_gen_dir/lit_exports.rollup.js" ]
  outputs = [ "$chrome_resources_dir/lit/index.js" ]
  deps = [ ":bundle_lit" ]
}

generate_grd("build_lit_grdp") {
  grd_prefix = "material_web_components"
  out_grd = "$target_gen_dir/lit_resources.grdp"
  input_files_base_dir = rebase_path(chrome_resources_dir, root_build_dir)
  resource_path_prefix = "mwc"

  input_files = [
    "tslib/tslib.js",
    "lit/index.js",
  ]

  deps = [
    ":copy_lit_bundle",
    ":copy_tslib",
  ]
}

generate_grd("build_mwc_grdp") {
  grd_prefix = "material_components"
  out_grd = "$target_gen_dir/mwc_resources.grdp"
  input_files_base_dir = rebase_path(chrome_resources_dir, root_build_dir)
  resource_path_prefix = "mwc"

  input_files = material_files

  deps = [ ":mwc_rewrite" ]
}

