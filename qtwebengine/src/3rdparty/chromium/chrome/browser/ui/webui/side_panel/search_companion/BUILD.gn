import("//mojo/public/tools/bindings/mojom.gni")

# Feature dependent on side panel only available on desktop.
assert(!is_android)

mojom("mojo_bindings") {
  sources = [ "search_companion.mojom" ]
  webui_module_path = "/"
  use_typescript_sources = true
  public_deps = [
    "//mojo/public/mojom/base",
    "//ui/base/mojom",
    "//ui/gfx/geometry/mojom",
    "//url/mojom:url_mojom_gurl",
  ]
}
