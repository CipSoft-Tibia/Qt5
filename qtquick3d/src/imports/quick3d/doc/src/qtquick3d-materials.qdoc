/*!
    \qmltype FrostedGlassMaterial
    \inqmlmodule QtQuick3D.Materials
    \brief Frosted Glass material.

    \image material_frosted_glass.png

    The frosted glass material has the following properties:
    \table
    \header
        \li Property Name
        \li Property Type
        \li Description
        \row
            \li roughness
            \li real
            \li Roughness of the material. Range is \c {[0, 1]}, with \c 0 being totally reflective.
            Default is \c {0}.
        \row
            \li blur_size
            \li real
            \li Amount of blurring behind the glass. Range is \c {[0, 50]}. Default is \c {8.0}.
        \row
            \li uEnvironmentMappingEnabled
            \li bool
            \li Does the material use an environment map. Default is \c {true}.
        \row
            \li uShadowMappingEnabled
            \li bool
            \li Does the material use a baked shadow map. Default is \c {false}.
        \row
            \li glass_bfactor
            \li real
            \li Glass bump map strength. Default is \c {0.0}.
        \row
            \li glass_binside
            \li bool
            \li Should the bump map be used only for the internal lighting. Default is \c {false}.
        \row
            \li uFresnelPower
            \li real
            \li Fresnel power of the material. Default is \c {1.0}.
        \row
            \li reflectivity_amount
            \li real
            \li Reflectivity of the material. Range is \c {[0, 1]}, with \c 1 being totally
            reflective. Default is \c {1}.
        \row
            \li glass_ior
            \li real
            \li Index of refraction for the material. Sensible range for different types of
            glass is \c {[1.4, 2.1]}. Default is \c {1.5}.
        \row
            \li intLightFall
            \li real
            \li Light intensity falloff rate. Default is \c {2.0}.
        \row
            \li intLightRot
            \li real
            \li The angle of lightsource. Band is perpendicular to this. Range is \c {[0, 360]}.
            Default is \c {0}.
        \row
            \li intLightBrt
            \li real
            \li Brightness of the band-light. Default is \c {0}.
        \row
            \li bumpScale
            \li real
            \li Scale of the bump bands. Range is \c {[0, 5]}. Default is \c {0.5}.
        \row
            \li bumpBands
            \li int
            \li Number of the bump bands. Range is \c {[0, 10]}. Default is \c {1}.
        \row
            \li bumpCoords
            \li vector3d
            \li Bump coordinates of the refraction. Default is \c {Qt.vector3d(1, 1, 1)}.
        \row
            \li intLightPos
            \li vector2d
            \li Position of the band-light in the UV space. Default is \c {Qt.vector2d(0.5, 0)}.
        \row
            \li glass_color
            \li vector3d
            \li Base color of the material. Default is \c {Qt.vector3d(0.9, 0.9, 0.9)}.
        \row
            \li intLightCol
            \li vector3d
            \li Color of the band-light. Default is \c {Qt.vector3d(0.9, 0.9, 0.9)}.
        \row
            \li glass_bump
            \li TextureInput
            \li Additional bump texture map for the surface. Default is a \l Texture with source
            \c {"maps/spherical_checker.png"}.
        \row
            \li uEnvironmentTexture
            \li TextureInput
            \li The environment texture map. Enabled when \c uEnvironmentMappingEnabled is
            \c {true}. Default is a \l Texture with source \c {"maps/spherical_checker.png"}.
        \row
            \li uBakedShadowTexture
            \li TextureInput
            \li The baked shadow texture map. Enabled when \c uShadowMappingEnabled is
            \c {true}. Default is a \l Texture with source \c {"maps/shadow.png"}.
        \row
            \li randomGradient1D
            \li TextureInput
            \li A texture map used to create the random bumpiness of the material together with
            randomGradient2D, randomGradient3D, and randomGradient4D. Default is a
            \l Texture with source \c {"maps/randomGradient1D.png"}.
        \row
            \li randomGradient2D
            \li TextureInput
            \li A texture map used to create the random bumpiness of the material together with
            randomGradient1D, randomGradient3D, and randomGradient4D. Default is a
            \l Texture with source \c {"maps/randomGradient3D.png"}.
        \row
            \li randomGradient3D
            \li TextureInput
            \li A texture map used to create the random bumpiness of the material together with
            randomGradient1D, randomGradient2D, and randomGradient4D. Default is a
            \l Texture with source \c {"maps/randomGradient3D.png"}.
        \row
            \li randomGradient4D
            \li TextureInput
            \li A texture map used to create the random bumpiness of the material together with
            randomGradient1D, randomGradient2D, and randomGradient3D. Default is a
            \l Texture with source \c {"maps/randomGradient4D.png"}.
    \endtable
*/

